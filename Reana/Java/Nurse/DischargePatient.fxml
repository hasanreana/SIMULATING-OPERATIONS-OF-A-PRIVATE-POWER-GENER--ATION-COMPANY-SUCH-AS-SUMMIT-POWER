            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="112.0" layoutY="74.0" text="Click on patient and click discharge to discharge them" />
      <TextField fx:id="patientIDsearchField" layoutX="81.0" layoutY="105.0" promptText="search patient ID" />
